{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off"
  },
  // y por ultimo, en la llave de "plugins" a√±ade el plugin de eslint para prettier
  "plugins": [
    "prettier"
  ]
}